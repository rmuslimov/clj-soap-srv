   <soapenv:Header>
      <auth:authentication>
         <auth:userid>user</auth:userid>
         <auth:password>password</auth:password>
      </auth:authentication>
   </soapenv:Header>
   <soapenv:Body>
      <ns:ping>
         <ns:OTA_PingRQ Version="1" EchoToken="abcd">
            <ns:EchoData>ping</ns:EchoData>
         </ns:OTA_PingRQ>
      </ns:ping>
   </soapenv:Body>
</soapenv:Envelope>
